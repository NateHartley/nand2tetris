/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=sel , b=b , out=Andbsel );
    Not(in=sel , out=Notsel );
    And(a=a , b=Notsel , out=NotselAnda );
    Or(a=Andbsel , b=NotselAnda , out=out );
}